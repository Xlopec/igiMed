<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="maxoliynick.igimed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="roboguice.modules"
            android:value="maxoliynick.igimed.di.RoboBindings" />

        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.CreateEmployeeActivity"
            android:label="@string/create_employee_title" />

        <activity
            android:name=".view.EditEmployeeActivity"
            android:label="@string/edit_employee_title" />

    </application>

</manifest>